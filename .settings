[editor]
indent_with_tabs = false
;vibrant-ink
;rubyblue
;mbo
;pasteldark
;twilight
theme = rubyblue
font_size = 14
linting = true
key_toggleComment = Cmd+U
show_trailing_space = true
tab_size = 4

[code-beautifier]
indent_level = 4
indent_with_tabs = false
preserve_newlines = true
max_preserve_newlines = 2
space_before_conditional = false

[terminal]
font_size = 14
font_color = white
background_color = #232830
cursor_color = gray